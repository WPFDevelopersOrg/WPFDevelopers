							<Storyboard x:Key="StarStoryboard" RepeatBehavior="Forever">
								<DoubleAnimation Storyboard.TargetName="PART_Ellipse" Storyboard.TargetProperty="(UIElement.RenderTransform).(RotateTransform.Angle)" From="0" To="360" Duration="0:0:1.0" />
