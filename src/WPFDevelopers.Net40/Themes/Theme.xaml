							<Trigger.ExitActions>
								<StopStoryboard BeginStoryboardName="Storyboard_LoadRunning" />
							</Trigger.ExitActions>
						</Trigger>
					</ControlTemplate.Triggers>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
