						</Storyboard>
					</ControlTemplate.Resources>
					<Grid>
						<Viewbox>
							<Grid Width="400" Height="400">
								<Grid x:Name="Part_Cycle_Rotate1" RenderTransformOrigin="0.5,0.5">
									<Grid.RenderTransform>
										<TransformGroup>
											<ScaleTransform />
											<SkewTransform />
											<RotateTransform />
											<TranslateTransform />
										</TransformGroup>
