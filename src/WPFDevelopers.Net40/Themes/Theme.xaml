										</PathFigure>
									</PathGeometry>
								</Path.Data>
							</Path>
							<Path Stroke="{TemplateBinding Background}" StrokeThickness="{TemplateBinding StrokeThickness}">
								<Path.Data>
									<PathGeometry>
