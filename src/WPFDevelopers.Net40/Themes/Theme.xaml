											</Setter>
											<Setter TargetName="PART_Ellipse" Property="ToolTip" Value="展开" />
										</Trigger>
									</ControlTemplate.Triggers>
								</ControlTemplate>
							</ToggleButton.Template>
						</ToggleButton>
					</controls:SmallPanel>
				</ControlTemplate>
			</Setter.Value>
