				<Path x:Name="PART_Path" Grid.Column="4" Width="10" Height="12" VerticalAlignment="Center" Data="{StaticResource WD.MenuItemGeometry}" Fill="{TemplateBinding Foreground}" FlowDirection="LeftToRight" Stretch="Fill" />
