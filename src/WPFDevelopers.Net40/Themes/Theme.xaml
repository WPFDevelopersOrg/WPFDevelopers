								<Button.Content>
									<Path Width="15" Height="15" Data="{StaticResource WD.ReplaceGeometry}" Fill="{StaticResource WD.PrimaryNormalSolidColorBrush}" Stretch="Fill" />
								</Button.Content>
							</Button>
							<Button x:Name="PART_AddButton" Width="40" Height="40" Style="{StaticResource WD.PathButton}" ToolTip="选择图片">
								<Button.Content>
									<Path Width="20" Height="20" Data="{StaticResource WD.AddGeometry}" Fill="{StaticResource WD.PrimaryNormalSolidColorBrush}" IsHitTestVisible="False" RenderTransformOrigin="0.5,0.5" Stretch="Fill">
